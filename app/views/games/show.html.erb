<% provide(:title, @game.title) %>
<div class="container">
<h3>
  <%= image_for @game %>
  <%= @game.title %>
</h3>
<div class="row"> 
<h5>Console: <small><%= @game.console %></small></h5>
<h5>Type: <small><%= @game.genre %></small></h5>
<h5>Publisher: <small><%= @game.publisher %></small></h5>
<h5>Developer: <small><%= @game.developer %></small></h5>
</div>
</div>